{
  "app": {
    "background": {
      "scripts": [
          "lib/jquery.min.js",
          "lib/buy.js",
          "class/IndexedDB.js",
          "class/Sync.js",
          "class/Store.js",
          "class/Notes.js",
          "background/general.js",
          "background/notifications.js",
          "background/background.js"
        ]
    }
  },
  "author": "ProWebJect - Michal Gora",
  "description": "Sticky Notes for your desktop! It is easy to manage and customize.",
  "icons": {
    "128": "img/icon_128.png",
    "19": "img/icon_19.png",
    "38": "img/icon_38.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "35",
  "name": "Sticky Notes",
  "offline_enabled": true,
  "optional_permissions": [
    "audioCapture",
    "background"
  ],
  "permissions": [
    "notifications",
    "storage",
    "syncFileSystem",
    {
      "fileSystem": [
        "write",
        "retainEntries"
      ]
    },
    "identity",
    "alwaysOnTopWindows",
    "tts",
    "clipboardWrite",
    "clipboardRead",
    "webview",
    "http://prowebject.com/*",
    "https://www.paypalobjects.com/*"
  ],
  "short_name": "Sticky Notes",
  "version": "1.17.0.0"
}
